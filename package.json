{
  "name": "sistema-vitana",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "dev:server": "cd server && npm run dev",
    "dev:full": "concurrently --kill-others \"npm run dev:server\" \"sleep 3 && npm run dev\"",
    "dev:full": "concurrently --kill-others \"npm run dev:server\" \"wait-on http://localhost:3001/health && npm run dev\"",
    "build": "tsc --noEmit && vite build",
    "build:prod": "tsc --noEmit && vite build --mode production",
    "build:railway": "tsc --noEmit && vite build --mode production",
    "start": "node server/server.js",
    "preview": "vite preview",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "postbuild": "echo 'Build completed successfully'",
    "railway:build": "npm run build:prod",
    "railway:start": "npm start",
    "server:start": "cd server && node server.js",
    "server:dev": "cd server && npm run dev",
    "setup": "npm install && cd server && npm install",
    "clean": "rm -rf node_modules server/node_modules dist",
    "reset": "npm run clean && npm run setup"
  },
  "dependencies": {
    "@emailjs/browser": "^4.3.3",
    "@zxing/library": "^0.21.3",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}